{
    "name": "@crownstack/api",
    "version": "0.0.1",
    "private": true,
    "description": "Crown Stack Api",
    "main": "dist/server.js",
    "types": "dist/server.d.ts",
    "scripts": {
        "test": "jest --config jest.test.json --colors --forceExit --detectOpenHandles --runInBand",
        "integration": "jest --config jest.intg.json --colors --forceExit --detectOpenHandles --passWithNoTests",
        "coverage": "nyc npm run test",
        "tsc": "tsc",
        "build:ts": "ts-node src/server.ts",
        "build": "npm run clean && node_modules/typescript/bin/tsc",
        "start": "nodemon build:ts",
        "migrate": "node_modules/.bin/sequelize db:migrate --env development",
        "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo --env development",
        "migrate:undo:all": "node_modules/.bin/sequelize db:migrate:undo:all --env development"
    },
    "author": "Manish Rai",
    "license": "UNLICENSED",
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "@types/bcryptjs": "^2.4.2",
        "@types/bluebird": "^3.5.29",
        "@types/node": "^13.5.2",
        "@types/validator": "^12.0.1",
        "bcrypt": "^4.0.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "class-validator": "^0.11.0",
        "cors": "^2.8.5",
        "dotenv": "^7.0.0",
        "express": "^4.16.4",
        "express-rate-limit": "^5.1.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "nodemon": "^1.18.10",
        "pg": "^7.18.1",
        "pg-hstore": "^2.3.3",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^5.21.3",
        "sequelize-cli-typescript": "^3.2.0-c",
        "sequelize-hierarchy": "^2.0.4",
        "sequelize-typescript": "^1.1.0",
        "sync-request": "^6.0.0",
        "typescript": "3.6.3"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/bcrypt": "^3.0.0",
        "@types/chai": "^4.2.11",
        "@types/cors": "^2.8.4",
        "@types/express": "^4.16.1",
        "@types/jest": "^24.0.17",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/mocha": "^7.0.2",
        "@types/swagger-ui-express": "^3.0.0",
        "jest": "24.8.0",
        "mocha": "^7.1.2",
        "nyc": "^15.0.1",
        "source-map-support": "^0.5.19",
        "swagger-ui-express": "^4.0.2",
        "ts-jest": "^24.0.0",
        "ts-node": "^8.10.1"
    }
}
